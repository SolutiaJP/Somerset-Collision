<?xml version="1.0" encoding="UTF-8"?><widget xmlns = "http://www.w3.org/ns/widgets"	xmlns:gap = "http://phonegap.com/ns/1.0"	id        = "com.solutiaconsulting.seedapp"	versionCode=""	version   = "1.0.15">	<name>Seed App</name>	<description>Seed App Description</description>	<author href="https://build.phonegap.com" email="tom_schreck@solutiaconsulting.com">Tom Schreck</author>		<preference name="phonegap-version" value="3.7.0" />	<preference name="permissions" value="none" />	<preference name="orientation" value="portrait" />	<preference name="target-device" value="handset" />	<preference name="fullscreen" value="true" />	<preference name="webviewbounce" value="true" />	<preference name="prerendered-icon" value="true" />	<preference name="stay-in-webview" value="true" />	<preference name="ios-statusbarstyle" value="black-opaque" />	<preference name="detect-data-types" value="true" />	<preference name="exit-on-suspend" value="false" />	<preference name="show-splash-screen-spinner" value="true" />	<preference name="auto-hide-splash-screen" value="true" />	<preference name="disable-cursor" value="false" />	<preference name="android-minSdkVersion" value="19" />	<preference name="android-targetSdkVersion" value="19" />	<preference name="android-installLocation" value="auto" />	<preference name="KeyboardDisplayRequiresUserAction" value="false" />		<preference name="EnableViewportScale" value="true" />	<preference name="MediaPlaybackRequiresUserAction" value="false" />	<preference name="AllowInlineMediaPlayback" value="false" />	<preference name="BackupWebStorage" value="none" />	<preference name="TopActivityIndicator" value="gray" />		<preference name="HideKeyboardFormAccessoryBar" value="false" />	<preference name="SuppressesIncrementalRendering" value="false" />	<preference name="SplashScreenDelay" value="5000" />	<preference name="ErrorUrl" value=""/>	<preference name="BackgroundColor" value="0x000000"/>	<preference name="KeepRunning" value="true"/>	<preference name="DisallowOverscroll" value="false"/>	<preference name="LoadingDialog" value=","/>	<preference name="LoadUrlTimeoutValue" value="20000" />		<gap:platform name="ios" />	<gap:platform name="android" />	<!--ITUNES-->	<icon src="res/ios/itunes/iTunesArtwork" gap:platform="ios" width="512" height="512" />	<icon src="res/ios/itunes/iTunesArtwork@2x"	gap:platform="ios" width="1024" height="1024" />	<!--iOS APP ICON PORTRAIT-->	<icon src="res/ios/icon/appIcon-29.png"	gap:platform="ios" width="29" height="29" />	<icon src="res/ios/icon/appIcon-29@2x.png" gap:platform="ios" width="58" height="58" />	<icon src="res/ios/icon/appIcon-40.png"	gap:platform="ios" width="40" height="40" />	<icon src="res/ios/icon/appIcon-40@2x.png" gap:platform="ios" width="80" height="80" />	<icon src="res/ios/icon/appIcon-50.png"	gap:platform="ios" width="50" height="50" />	<icon src="res/ios/icon/appIcon-50@2x.png" gap:platform="ios" width="100" height="100" />	<icon src="res/ios/icon/appIcon-57.png"	gap:platform="ios" width="57" height="57" />	<icon src="res/ios/icon/appIcon-57@2x.png" gap:platform="ios" width="114" height="114" />	<icon src="res/ios/icon/appIcon-60.png"	gap:platform="ios" width="60" height="60" />	<icon src="res/ios/icon/appIcon-60@2x.png" gap:platform="ios" width="120" height="120" />	<icon src="res/ios/icon/appIcon-60@3x.png" gap:platform="ios" width="180" height="180" />	<icon src="res/ios/icon/appIcon-72.png"	gap:platform="ios" width="72" height="72" />	<icon src="res/ios/icon/appIcon-72@2x.png" gap:platform="ios" width="144" height="144" />	<icon src="res/ios/icon/appIcon-76.png"	gap:platform="ios" width="76" height="76" />	<icon src="res/ios/icon/appIcon-76@2x.png" gap:platform="ios" width="152" height="152" />	<!--ANDROID APP ICON PORTRAIT-->	<icon src="res/android/icon/appIcon-36.png" gap:platform="android" gap:qualifer ="ldpi" />	<icon src="res/android/icon/appIcon-48.png" gap:platform="android" gap:qualifer ="mdpi" />	<icon src="res/android/icon/appIcon-72.png" gap:platform="android" gap:qualifer="hdpi" />	<icon src="res/android/icon/appIcon-96.png" gap:platform="android" gap:qualifer="xhdpi" />	<icon src="res/android/icon/appIcon-144.png" gap:platform="android" gap:qualifer="xxhdpi" />	<icon src="res/android/icon/appIcon-512.png" gap:platform="android" gap:qualifer="xhdpi" />	<!--iOS SPLASH SCREEN PORTRAIT-->	<gap:splash src="res/ios/splash/splashScreen-320-480.png" gap:platform="ios" width="320" height="480" />	<gap:splash src="res/ios/splash/splashScreen-320-480@2x.png" gap:platform="ios" width="640" height="960" />	<gap:splash src="res/ios/splash/splashScreen-640-1136.png" gap:platform="ios" width="640" height="1136" />	<gap:splash src="res/ios/splash/splashScreen-750-1334.png" gap:platform="ios" width="750" height="1334" />	<gap:splash src="res/ios/splash/splashScreen-1242-2208.png" gap:platform="ios" width="1242" height="2208" />	<!--ANDROID SPLASH SCREEN PORTRAIT-->	<gap:splash src="res/android/splash/splashScreen-ldpi-200-300.png" gap:platform="android" gap:qualifer="port-ldpi" />	<gap:splash src="res/android/splash/splashScreen-mdpi-320-480.png" gap:platform="android" gap:qualifer="port-mdpi" />	<gap:splash src="res/android/splash/splashScreen-hdpi-480-800.png" gap:platform="android" gap:qualifer="port-hdpi" />	<gap:splash src="res/android/splash/splashScreen-xhdpi-720-1280.png" gap:platform="android" gap:qualifer="port-xhdpi" />		<!--DEFAULT ICON-->	<icon src="icon.png" gap:role="default" />	<gap:splash src="splash.png" />	<!--PLUGINS-->	<gap:plugin name="org.apache.cordova.device" />	<gap:plugin name="org.apache.cordova.splashscreen" />	<gap:plugin name="org.apache.cordova.statusbar" />	<access origin="*" /></widget>